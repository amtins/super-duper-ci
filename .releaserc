{
  "branches": [
    {
      "name": "main",
      "channel": "latest"
    },
    {
      "name": "pre-release",
      "channel": "next",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    [
      "@semantic-release/release-notes-generator",
      {
        "header": "Super Duper CI changes",
        "presetConfig": {
          "types": [
            {
              "type": "feat",
              "section": "ğŸš€ New Features",
              "hidden": false
            },
            {
              "type": "fix",
              "section": "ğŸ› Enhancements and Bug Fixes",
              "hidden": false
            },
            {
              "type": "docs",
              "section": "ğŸ“– Docs",
              "hidden": false
            },
            {
              "type": "style",
              "section": "ğŸ¨ Styles",
              "hidden": false
            },
            {
              "type": "refactor",
              "section": "ğŸ”© Refactor",
              "hidden": false
            },
            {
              "type": "perf",
              "section": "âš¡ï¸ Performances",
              "hidden": false
            },
            {
              "type": "test",
              "section": "âœ… Tests",
              "hidden": false
            },
            {
              "type": "ci",
              "section": "ğŸ” CI",
              "hidden": false
            },
            {
              "type": "chore",
              "section": "ğŸ§¹ Chore",
              "hidden": false
            }
          ]
        },
        "writerOpts": {
          "groupBy": "type",
          "commitGroupsSort": [
            "feat",
            "fix",
            "docs",
            "style",
            "refactor",
            "perf",
            "test",
            "ci",
            "chore"
          ]
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "docs/CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/npm",
      {
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "package.json",
          "docs/CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "dist/custom-button.js",
            "label": "custom-button.js"
          },
          {
            "path": "dist/custom-button.es.js",
            "label": "custom-button.es.js"
          },
          {
            "path": "dist/custom-button.d.ts",
            "label": "custom-button.d.ts"
          }
        ]
      }
    ]
  ]
}
